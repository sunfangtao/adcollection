<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 填充 -->
    <solid android:color="#e6e6e6" /> <!-- 定义填充的颜色值 -->

    <!-- 描边 -->
    <stroke
        android:width="1dp"
        android:color="#dcdcdc" />

    <!-- 圆角 -->
    <corners android:radius="90dp" />

    <!-- 间隔  -->
    <padding
        android:bottom="3dp"
        android:left="20dp"
        android:right="20dp"
        android:top="3dp" />

</shape>
